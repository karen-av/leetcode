

def func_count(x, y):
    #if x * y % 2 == 0:
     #   return x * y / 2
    return x * y // 2 + x * y % 2

    #return(x * y / 2 if x * y % 2 == 0 else int(x*y - x * y/2 + x * y % 2))

  

